#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( crdt:Array
                crdt:Counter
                crdt:Map
                crdt:Text
                crdt:XML
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( crdt:Create
                crdt:Delete
                crdt:Read
                crdt:Synchronize
                crdt:Update
              )
] .


